{
  "build": {
    "distDir": "../../admin_core/dist",
    "devPath": "http://localhost:8080"
  },
  "ctx": {},
  "tauri": {
    "window": {
      "title": "Stract Admin",
      "width": 1280,
      "height": 720
    },
    "embeddedServer": {
      "active": true
    },
    "bundle": {
      "active": true
    },
    "whitelist": {
      "all": true
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "edge": {
      "active": true
    },
    "inliner": {
      "active": true
    }
  }
}
